Options -MultiViews
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.html [QSA,L]
RewriteCond %{REQUEST_URI} !\.(css|gif|ico|jpg|js|png|swf|txt|svg|woff|ttf|eot)

Header set X-Frame-Options DENY
Header set Content-Security-Policy "default-src 'self' *.opsera.io localhost:* *.fontawesome.com https://fonts.googleapis.com *.okta.com *.oktapreview.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' *.fontawesome.com https://cdn.jsdelivr.net; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdn.jsdelivr.net; img-src 'self' data: *.opsera.io; font-src 'self' data: *.opsera.io data: localhost:* *.fontawesome.com https://fonts.gstatic.com https://cdn.jsdelivr.net; worker-src 'self' blob:;"