{
  "name": "okta-client-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.jsx .",
    "format": "npm run prettier -- --write",
    "prettier": "prettier  \"**/*.+(js|md|ts|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
    "refresh-internal-libraries": "rm -rf node_modules/@opsera && npm install",
    "refresh-internal-libraries-windows": "rmdir /s /q node_modules\\@opsera && npm install",
    "update-lock-file": "rm -rf node_modules/@opsera && npm run update-opsera && npm install",
    "start": "PORT=8080 react-app-rewired start",
    "start-linux-mac": "npm run refresh-internal-libraries && PORT=8080 FAST_REFRESH=false react-scripts start",
    "start-analytics-dev-mac": "npm run refresh-internal-libraries && PORT=8080 REACT_APP_ANALYTICS_API_SERVER_URL='https://localhost:5005' FAST_REFRESH=false react-scripts start",
    "start-windows": "npm run refresh-internal-libraries-windows && set PORT=8080 && set FAST_REFRESH=false && react-scripts start",
    "start-analytics-dev-windows": "set PORT=8080 && set REACT_APP_ANALYTICS_API_SERVER_URL='https://localhost:5005' && set FAST_REFRESH=false && react-scripts start",
    "build:test": "env-cmd -f .env.test react-scripts build && mv build build-test",
    "build:staging": "env-cmd -f .env.staging react-scripts build && mv build build-staging",
    "build:freetrial": "env-cmd -f .env.freetrial react-scripts build && mv build build-freetrial",
    "build:kube-generic": "env-cmd -f .env.kube-generic react-scripts --max-old-space-size=4096 build && mv build build-kube-generic",
    "build:kube-dev": "env-cmd -f .env.kube-dev react-scripts build && mv build build-kube-dev",
    "build:kube-test": "env-cmd -f .env.kube-test react-scripts build && mv build build-kube-test",
    "build:kube-prod": "env-cmd -f .env.kube-prod react-scripts build && mv build build-kube-prod",
    "build:honeywell": "env-cmd -f .env.honeywell react-scripts build && mv build build-honeywell",
    "build:honeywell-sandbox": "env-cmd -f .env.honeywell-sandbox react-scripts build && mv build build-honeywell-sandbox",
    "build:production": "react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-nivo": "rm -rf node_modules/@nivo && npm install --force --save @nivo/core@latest @nivo/bar@latest @nivo/calendar@latest @nivo/circle-packing@latest @nivo/heatmap@latest @nivo/legends@latest @nivo/line@latest @nivo/pie@latest @nivo/scatterplot@latest",
    "update-okta": "npm install --save @okta/okta-auth-js@latest @okta/okta-react@latest @okta/okta-signin-widget@latest",
    "update-editorjs": "npm install --save @editorjs/editorjs@latest @editorjs/checklist@latest @editorjs/code@latest @editorjs/delimiter@latest @editorjs/header@latest @editorjs/inline-code@latest @editorjs/marker@latest @editorjs/nested-list@latest @editorjs/paragraph@latest @editorjs/paragraph@latest @editorjs/quote@latest @editorjs/table@latest",
    "update-fontawesome": "npm install --save @fortawesome/fontawesome-pro@latest @fortawesome/fontawesome-svg-core@latest @fortawesome/free-brands-svg-icons@latest @fortawesome/pro-light-svg-icons@latest @fortawesome/pro-regular-svg-icons@latest @fortawesome/pro-solid-svg-icons@latest @fortawesome/pro-thin-svg-icons@latest @fortawesome/react-fontawesome@latest",
    "update-opsera": "rm -rf node_modules/@opsera && npm install --save @opsera/definitions@latest @opsera/dhx-suite-package@latest @opsera/know-your-role@latest @opsera/persephone@latest @opsera/react-faq-markdown@latest @opsera/react-vanity-set@latest"
  },
  "dependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.7.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.26.4",
    "@editorjs/header": "^2.6.2",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/nested-list": "^1.0.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/table": "^2.1.0",
    "@fortawesome/fontawesome-pro": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/pro-light-svg-icons": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/pro-solid-svg-icons": "^6.4.0",
    "@fortawesome/pro-thin-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@microlink/react-json-view": "^1.22.2",
    "@monaco-editor/react": "^4.4.6",
    "@nivo/bar": "^0.80.0",
    "@nivo/calendar": "^0.80.0",
    "@nivo/circle-packing": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/heatmap": "^0.80.0",
    "@nivo/legends": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@nivo/scatterplot": "^0.80.0",
    "@okta/okta-auth-js": "^6.7.2",
    "@okta/okta-react": "^6.7.0",
    "@okta/okta-signin-widget": "^6.9.0",
    "@opsera/definitions": "latest",
    "@opsera/dhx-suite-package": "^7.1.2",
    "@opsera/know-your-role": "latest",
    "@opsera/persephone": "latest",
    "@opsera/react-faq-markdown": "latest",
    "@opsera/react-vanity-set": "latest",
    "ace-builds": "^1.18.0",
    "assert": "^2.0.0",
    "axios": "^0.26.1",
    "bootstrap": "^4.6.2",
    "buffer": "^6.0.3",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.29.3",
    "diff-match-patch": "^1.0.5",
    "env-cmd": "^10.1.0",
    "fast-xml-parser": "^4.2.2",
    "https-browserify": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.28",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-app-rewired": "^2.2.1",
    "react-bootstrap": "^1.6.6",
    "react-csv": "^2.2.2",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-editor-js": "^2.1.0",
    "react-json-editor-ajrm": "2.5.13",
    "react-lineto": "^3.3.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^4.0.3",
    "react-select": "^5.7.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-table": "^7.8.0",
    "react-widgets": "^5.8.4",
    "react-widgets-moment": "^5.0.20",
    "react-widgets-simple-number": "^5.0.20",
    "sass": "^1.62.0",
    "socket.io-client": "^4.6.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "text-encoding": "^0.7.0",
    "web-vitals": "^2.1.4",
    "websocket": "^1.0.34"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=16.5.0",
    "npm": ">=7.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1"
  }
}
