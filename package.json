{
  "name": "okta-client-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false && react-app-rewired build",
    "build:freetrial": "env-cmd -f .env.freetrial react-app-rewired build && mv build build-freetrial",
    "build:honeywell": "env-cmd -f .env.honeywell react-app-rewired build && mv build build-honeywell",
    "build:honeywell-sandbox": "env-cmd -f .env.honeywell-sandbox react-app-rewired build && mv build build-honeywell-sandbox",
    "build:kube-dev": "env-cmd -f .env.kube-dev react-app-rewired build && mv build build-kube-dev",
    "build:kube-generic": "env-cmd -f .env.kube-generic react-app-rewired build && mv build build-kube-generic",
    "build:kube-prod": "env-cmd -f .env.kube-prod react-app-rewired build && mv build build-kube-prod",
    "build:kube-test": "env-cmd -f .env.kube-test react-app-rewired build && mv build build-kube-test",
    "build:production": "react-app-rewired build",
    "build:staging": "env-cmd -f .env.staging react-app-rewired build && mv build build-staging",
    "build:test": "env-cmd -f .env.test react-app-rewired build && mv build build-test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js,.jsx .",
    "format": "npm run prettier -- --write",
    "prettier": "prettier  \"**/*.+(js|md|ts|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
    "refresh-internal-libraries": "rm -rf node_modules/@opsera && npm install",
    "start": "PORT=8080 react-app-rewired start",
    "start-analytics-dev-mac": "FAST_REFRESH=false && npm run refresh-internal-libraries && PORT=8080 REACT_APP_ANALYTICS_API_SERVER_URL='https://localhost:5005' react-app-rewired start",
    "start-analytics-dev-windows": "set PORT=8080 && set REACT_APP_ANALYTICS_API_SERVER_URL='https://localhost:5005' && set FAST_REFRESH=false && react-app-rewired start",
    "start-linux-mac": "FAST_REFRESH=false && npm run refresh-internal-libraries && PORT=8080 react-app-rewired start",
    "start-windows": "set PORT=8080 && set FAST_REFRESH=false && react-app-rewired start",
    "test": "react-app-rewired test",
    "update-lock-file": "rm -rf node_modules/@opsera && rm -rf package-lock.json && npm install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
